<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:wsdli="http://www.w3.org/2004/08/wsdl-instance"
  targetNamespace="http://frascati.ow2.org/helloworld-ws"
  name="helloworld-ws-server">

  <service name="s" promote="server/s">
    <interface.java interface="org.ow2.frascati.examples.helloworld.annotated.HelloService" />
    <binding.ws uri="http://localhost:9000/HelloService"/>
  </service>

  <component name="server">
    <implementation.java class="org.ow2.frascati.examples.helloworld.annotated.ServerImpl" />
    <service name="s">
      <interface.java interface="org.ow2.frascati.examples.helloworld.annotated.HelloService" />
    </service>
  </component>

</composite>
