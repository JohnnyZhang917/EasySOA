      <binding.ws name="webService" wsdlElement="http://clients.pureairflowers/#wsdl.port(PureAirFlowers/PureAirFlowersClientsImplPort)" instance:wsdlLocation="http://localhost:9000/PureAirFlowersClientsLocalTest?wsdl"/>
    </reference>
  </component>
