  <component name="ProxyUnused">
    <implementation.java class="org.easysoa.sca.proxy.RestSoapProxyImpl"/>
    <service name="restInterface">
      <interface.java interface="org.easysoa.sca.proxy.RestSoapProxy"/>
    </service>
    <reference name="ws">
