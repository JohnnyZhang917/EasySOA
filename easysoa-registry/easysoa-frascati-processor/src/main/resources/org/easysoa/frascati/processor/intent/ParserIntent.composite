<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" name="parser-intent">
  
  <service name="intent" promote="parser-intent/intent">  
       <interface.java interface="org.ow2.frascati.tinfi.api.IntentHandler"/>
  </service>
  
  <service name="intent-observable" promote="parser-intent/intent-observable">
      	<interface.java interface="org.easysoa.frascati.processor.intent.ParserIntentObservable"/>
  </service>
  
  <component name="parser-intent">  
  
    <service name="intent">
    	<interface.java interface="org.ow2.frascati.tinfi.api.IntentHandler"/>
    </service> 
         
  	<service name="intent-observable">
      	<interface.java interface="org.easysoa.frascati.processor.intent.ParserIntentObservable"/>
  	</service>
              	       
    <implementation.java class="org.easysoa.frascati.processor.intent.ParserIntent"/>
    
  </component>
  
</composite>