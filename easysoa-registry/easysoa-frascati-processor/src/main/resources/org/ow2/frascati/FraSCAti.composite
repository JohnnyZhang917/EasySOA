name="org.ow2.frascati.FraSCAti">
 
   <component name="parser-intent">  
      
   	  <service name="intent-observable">
      	<interface.java interface="org.easysoa.frascati.processor.intent.ParserIntentObservable"/>
      </service> 
             	       
   	  <service name="intent">
