<?xml version="1.0" encoding="UTF-8"?>
<!--xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"-->
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" 
	xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
	xmlns:tuscany="http://tuscany.apache.org/xmlns/sca/1.0" 
	xmlns:instance="http://www.w3.org/2004/08/wsdl-instance" name="eventPafSample" targetNamespace="http://docs.oasis-open.org/ns/opencsa/sca/200912">
	
	<!-- minimal Discovery proxy with Event handler -->
	<include name="eventHandlerTest"/>
	
	<component name="glueComponent">
     	<implementation.java class="org.openwide.easysoa.test.event.paf.test.Glue" />	
        <service name="paf" >
        	<interface.java interface="org.openwide.easysoa.test.event.paf.test.PureAirFlowersService" />	
      		<binding.ws uri="http://localhost:8090/glue"/><!-- 8080 but nuxeo -->
        </service>
	   
	   <reference name="trip">
	      <interface.java interface="org.openwide.easysoa.test.event.paf.test.TripPortType"/>
	      <!-- binding.ws uri="http://localhost:9000/GalaxyTrip" -->
	      <!-- binding.ws 	wsdlElement ="http://test.paf.event.test.easysoa.openwide.org/#wsdl.port(GalaxyTrip/TripPort)" 
	      		     	wsdli:wsdlLocation="http://localhost:9002/GalaxyTrip?wsdl" / -->
	      <binding.ws wsdlElement ="http://test.paf.event.test.easysoa.openwide.org/#wsdl.port(Trip/TripPort)" 
	      		      	wsdli:wsdlLocation="http://localhost:9000/GalaxyTrip?wsdl" />
	    </reference>
	</component>


</composite>

