			<version>1.5-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-binding-rest</artifactId>
			<version>1.5-SNAPSHOT</version>
		</dependency>		
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-runtime-factory</artifactId>
			<version>1.5-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
