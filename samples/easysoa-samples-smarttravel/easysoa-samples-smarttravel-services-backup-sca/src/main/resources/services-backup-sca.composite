<?xml version="1.0" encoding="ISO-8859-15"?>
<composite name="serviceBackup" xmlns="http://www.osoa.org/xmlns/sca/1.0"
	xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1">

	<component name="meteoComponentBackup">
		<implementation.java class="net.webservicex.GlobalWeatherSoapImpl1" />
		<service name="meteoServiceBackup">
			<interface.wsdl interface="globalweather.asmx.wsdl#wsdl.interface(GlobalWeatherSoap)" />
			<binding.ws uri="http://localhost:9020/WeatherService" wsdlElement="http://www.webserviceX.NET#wsdl.port(GlobalWeather/GlobalWeatherSoap)" />
		</service>
	</component>

	<component name="currencyComponentBackup">
		<implementation.java class="de.daenet.webservices.currencyserver.CurrencyServerWebServiceSoapImpl1" />
		<service name="currencyServiceBackup">
			<interface.wsdl interface="currencyserverwebservice.asmx.wsdl#wsdl.interface(CurrencyServerWebServiceSoap)" />
			<binding.ws uri="http://localhost:9020/CurrencyService" wsdlElement="http://www.daenet.de/webservices/CurrencyServer#wsdl.port(CurrencyServerWebService/CurrencyServerWebServiceSoap)" />
		</service>
	</component>
	
	<component name="translateComponentBackup">
		<implementation.java class="com.microsofttranslator.api.v1.soap_svc.LanguageServiceImpl" />
		<service name="translateServiceBackup">
			<interface.wsdl interface="microsoftTranslatorWebService.svc.wsdl#wsdl.interface(LanguageService)" />
			<binding.ws uri="http://localhost:9020/TranslateService" wsdlElement="http://api.microsofttranslator.com/v1/soap.svc#wsdl.port(SoapService/BasicHttpBindingLanguageService)" />
		</service>
	</component>

</composite>