<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.easysoa.samples.smarttravel</groupId>
	<artifactId>easysoa-samples-smarttravel-test</artifactId>
	<name>EasySOA Sample Smart Travel Tests</name>
	<packaging>jar</packaging>

	<parent>
		<groupId>org.easysoa.samples</groupId>
		<artifactId>easysoa-samples-parent</artifactId>
		<version>0.3-SNAPSHOT</version>
		<relativePath>../easysoa-samples-parent/pom.xml</relativePath>
	</parent>

	<dependencies>
	    <dependency>
	    	<groupId>org.easysoa</groupId>
	    	<artifactId>easysoa-common</artifactId>
	    	<version>${project.version}</version>
			<scope>test</scope>
	    </dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-binding-factory</artifactId>
			<version>1.4</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-implementation-fractal</artifactId>
			<version>1.4</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-binding-ws</artifactId>
			<version>1.4</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-binding-http</artifactId>
			<version>1.4</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ow2.frascati</groupId>
			<artifactId>frascati-binding-rest</artifactId>
			<version>1.4</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.jdt</groupId>
			<artifactId>core</artifactId>
			<version>3.3.0.771</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.easysoa.registry</groupId>
			<artifactId>easysoa-registry-core</artifactId>
			<version>0.3-SNAPSHOT</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.easysoa.registry</groupId>
			<artifactId>easysoa-registry-rest</artifactId>
			<version>0.3-SNAPSHOT</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.easysoa.proxy</groupId>
			<artifactId>easysoa-proxy-core-httpdiscoveryproxy</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.easysoa.samples.smarttravel</groupId>
			<artifactId>easysoa-samples-smarttravel-trip</artifactId>
			<version>0.3-SNAPSHOT</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>jetty</groupId>
			<artifactId>org.mortbay.jetty.plus</artifactId>
			<version>5.1.10</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		
	    <!-- Spring deps required to have cxf config file -->
	    <dependency>
	      <groupId>org.springframework</groupId>
	      <artifactId>spring-core</artifactId>
	      <version>3.0.4.RELEASE</version>
	      <scope>test</scope>
	    </dependency>
	    <dependency>
	      <groupId>org.springframework</groupId>
	      <artifactId>spring-beans</artifactId>
	      <version>3.0.4.RELEASE</version>
	      <scope>test</scope>      
	    </dependency>
	    <dependency>
	      <groupId>org.springframework</groupId>
	      <artifactId>spring-context</artifactId>
	      <version>3.0.4.RELEASE</version>
	      <scope>test</scope>      
	    </dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
				</plugin>
		</plugins>
	</build>

</project>
