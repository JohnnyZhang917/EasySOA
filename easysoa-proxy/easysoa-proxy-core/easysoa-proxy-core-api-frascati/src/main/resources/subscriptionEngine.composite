<?xml version="1.0" encoding="UTF-8"?>
<sca:composite xmlns:sca="http://www.osoa.org/xmlns/sca/1.0" xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1" name="subscriptionEngine">
    <sca:component name ="subscriptionWebserviceComponent">

        <sca:service name="ISubscriptionWebService">
            <sca:interface.java interface="org.easysoa.subscriptions.handlers.ISubscriptionWebService"/>
            <frascati:binding.rest uri="http://localhost:8084/SubscriptionWebService/" />
        </sca:service>

        <sca:implementation.java class="org.easysoa.subscriptions.handlers.SubscriptionWebserviceImpl" ></sca:implementation.java>

        <sca:reference name="eventMessageHandler" target="eventMessageHandlerComponent" >
            <sca:interface.java interface="org.easysoa.exchangehandler.MessageHandler" />
        </sca:reference> 
 
    </sca:component>
        
</sca:composite>