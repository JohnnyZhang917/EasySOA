script(type='text/javascript')
  window.selectedId = 0

#contents(style='height: 650px')
  
  #breadcrumbs
    a.fake(href='#') Environment Master > Validation
      
  include navtree
  
  #doccontents
    h1 Environment 
      b Master
      
    .contentbox
      .title Validation summary
      
      table
        tr
          th(style='min-width: 200px') Service name
          th State
          th(style='min-width: 200px') Message
          th Last validation
          th Actions
        each n in new Array(7)
          tr
            td Client service
            td
              img(src='img/green.png')
            td OK
            td Yesterday, 11:00
            td
              img(src='img/run.png', style='margin-right: 5px')
              img(src='img/settings.png')
        tr
          td MyBuggyService
          td
            img(src='img/red.png')
          td Service is unavailable.
          td Yesterday, 11:00
          td
            img(src='img/run.png', style='margin-right: 5px')
            img(src='img/settings.png')
include lefttabs_env
